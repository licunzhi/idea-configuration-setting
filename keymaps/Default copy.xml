<keymap version="1" name="Default copy" parent="$default">
  <action id="EvaluateExpression">
    <keyboard-shortcut first-keystroke="alt f8" />
    <keyboard-shortcut first-keystroke="shift alt l" />
  </action>
  <action id="SplitHorizontally">
    <mouse-shortcut keystroke="alt button1 doubleClick" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt z" />
  </action>
  <action id="context.load" />
</keymap>